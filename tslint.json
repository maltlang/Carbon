{
  "extends": [
    "tslint-config-alloy",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "**/node_modules/**"
    ]
  },
  "rules": {
    "semicolon": [
      false,
      "always"
    ],
    "eofline": false,
    "indent": [
      true,
      "spaces",
      2
    ],
    "no-empty-interface": false,
    "jsdoc-format": false,
    "no-redundant-jsdoc": false
  }
}
